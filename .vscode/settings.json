{
  "workbench.colorCustomizations": {
    "gitDecoration.modifiedResourceForeground": "#E2C08D",
    "gitDecoration.stagedResourceForeground": "#81C784"
  },
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
